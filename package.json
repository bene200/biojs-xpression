{
  "name": "biojs-xpression",
  "description": "Visualise tissue-specific gene expression",
  "version": "0.1.3",
  "homepage": "https://github.com/bene200/biojs-xpression",
  "author": {
    "name": "Benedikt Rauscher",
    "email": "benedikt.rauscher@tgac.ac.uk"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bene200/biojs-xpression.git"
  },
  "bugs": {
    "url": "https://github.com/bene200/biojs-xpression/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/bene200/biojs-xpression/blob/master/LICENSE"
    }
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "echo 'Error: no test specified' && exit 1",
    "build": "mkdirp build && browserify -r ./:test -o build/test.js",
    "build-browser": "npm run build",
    "install": "npm run build",
    "watch": "watchify -r ./:test -v -o build/biojs-xpression.js",
    "w": "prunner \"npm run sniper\" \"npm run watch\"",
    "sniper": "sniper ."
  },
  "dependencies": {
    "backbone": "^1.2.1",
    "chroma-js": "^1.0.1",
    "d3": "^3.5.6",
    "jquery": "^2.1.4",
    "d3-tip": "^0.6.7"
  },
  "devDependencies": {
      "mkdirp": "^0.5.0",
      "browserify": "6.x",
      "sniper": "0.x",
      "watchify": "^1.0.6",
      "prunner": "1.x"
  },
  "sniper": {
    "js": [
      "/build/xpression.js"
    ],
    "css": [
      "/styles/style.css"
    ],
    "snippets": [
      "examples"
    ],
    "first": "simple"
  },
  "keywords": [
    "biojs",
    "expression",
    "gene expression",
    "tissue",
    "biojs"
  ]
}
